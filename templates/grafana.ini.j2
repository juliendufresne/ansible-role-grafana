{{ ansible_managed|comment }}
app_mode = {{ grafana_config__app_mode }}

[paths]
data    = {{ grafana_config__paths__data }}
logs    = {{ grafana_config__paths__logs }}
plugins = {{ grafana_config__paths__plugins }}

[server]
protocol         = {{ grafana_config__server__protocol }}
http_addr        = {{ grafana_config__server__http_addr }}
http_port        = {{ grafana_config__server__http_port }}
domain           = {{ grafana_config__server__domain }}
enforce_domain   = {{ grafana_config__server__enforce_domain }}
root_url         = {{ grafana_config__server__root_url }}
router_logging   = {{ grafana_config__server__router_logging }}
static_root_path = {{ grafana_config__server__static_root_path }}
enable_gzip      = {{ grafana_config__server__enable_gzip }}
cert_file        = {{ grafana_config__server__cert_file }}
cert_key         = {{ grafana_config__server__cert_key }}

[database]
type     = {{ grafana_config__database__type }}
host     = {{ grafana_config__database__host }}
name     = {{ grafana_config__database__name }}
user     = {{ grafana_config__database__user }}
password = {{ grafana_config__database__password }}
ssl_mode = {{ grafana_config__database__ssl_mode }}
path     = {{ grafana_config__database__path }}

[session]
provider          = {{ grafana_config__session__provider }}
provider_config   = {{ grafana_config__session__provider_config }}
cookie_name       = {{ grafana_config__session__cookie_name }}
cookie_secure     = {{ grafana_config__session__cookie_secure }}
session_life_time = {{ grafana_config__session__session_life_time }}

[analytics]
reporting_enabled      = {{ grafana_config__analytics__reporting_enabled }}
check_for_updates      = {{ grafana_config__analytics__check_for_updates }}
google_analytics_ua_id = {{ grafana_config__analytics__google_analytics_ua_id }}

[security]
admin_user                  = {{ grafana_config__security__admin_user }}
admin_password              = {{ grafana_config__security__admin_password }}
secret_key                  = {{ grafana_config__security__secret_key }}
login_remember_days         = {{ grafana_config__security__login_remember_days }}
cookie_username             = {{ grafana_config__security__cookie_username }}
cookie_remember_name        = {{ grafana_config__security__cookie_remember_name }}
disable_gravatar            = {{ grafana_config__security__disable_gravatar }}
data_source_proxy_whitelist = {{ grafana_config__security__data_source_proxy_whitelist|join(' ') }}

[snapshots]
external_enabled       = {{ grafana_config__snapshots__external_enabled }}
external_snapshot_url  = {{ grafana_config__snapshots__external_snapshot_url }}
external_snapshot_name = {{ grafana_config__snapshots__external_snapshot_name }}

[users]
allow_sign_up        = {{ grafana_config__users__allow_sign_up }}
allow_org_create     = {{ grafana_config__users__allow_org_create }}
auto_assign_org      = {{ grafana_config__users__auto_assign_org }}
auto_assign_org_role = {{ grafana_config__users__auto_assign_org_role }}
login_hint           = {{ grafana_config__users__login_hint }}

[auth.anonymous]
enabled  = {{ grafana_config__auth__anonymous__enabled }}
org_name = {{ grafana_config__auth__anonymous__org_name }}
org_role = {{ grafana_config__auth__anonymous__org_role }}

[auth.github]
enabled               = {{ grafana_config__auth__github__enabled }}
allow_sign_up         = {{ grafana_config__auth__github__allow_sign_up }}
client_id             = {{ grafana_config__auth__github__client_id }}
client_secret         = {{ grafana_config__auth__github__client_secret }}
scopes                = {{ grafana_config__auth__github__scopes }}
auth_url              = {{ grafana_config__auth__github__auth_url }}
token_url             = {{ grafana_config__auth__github__token_url }}
api_url               = {{ grafana_config__auth__github__api_url }}
team_ids              = {{ grafana_config__auth__github__team_ids }}
allowed_organizations = {{ grafana_config__auth__github__allowed_organizations }}

[auth.google]
enabled         = {{ grafana_config__auth__google__enabled }}
allow_sign_up   = {{ grafana_config__auth__google__allow_sign_up }}
client_id       = {{ grafana_config__auth__google__client_id }}
client_secret   = {{ grafana_config__auth__google__client_secret }}
scopes          = {{ grafana_config__auth__google__scopes }}
auth_url        = {{ grafana_config__auth__google__auth_url }}
token_url       = {{ grafana_config__auth__google__token_url }}
api_url         = {{ grafana_config__auth__google__api_url }}
allowed_domains = {{ grafana_config__auth__google__allowed_domains }}

[auth.proxy]
enabled         = {{ grafana_config__auth__proxy__enabled }}
header_name     = {{ grafana_config__auth__proxy__header_name }}
header_property = {{ grafana_config__auth__proxy__header_property }}
auto_sign_up    = {{ grafana_config__auth__proxy__auto_sign_up }}

[auth.basic]
enabled = {{ grafana_config__auth__basic__enabled }}

[auth.ldap]
enabled     = {{ grafana_config__auth__ldap__enabled }}
config_file = {{ grafana_config__auth__ldap__config_file }}

[smtp]
enabled      = {{ grafana_config__smtp__enabled }}
host         = {{ grafana_config__smtp__host }}
user         = {{ grafana_config__smtp__user }}
password     = {{ grafana_config__smtp__password }}
cert_file    = {{ grafana_config__smtp__cert_file }}
key_file     = {{ grafana_config__smtp__key_file }}
skip_verify  = {{ grafana_config__smtp__skip_verify }}
from_address = {{ grafana_config__smtp__from_address }}

[emails]
welcome_email_on_sign_up = {{ grafana_config__emails__welcome_email_on_sign_up }}

[log]
mode       = {{ grafana_config__log__mode }}
buffer_len = {{ grafana_config__log__buffer_len }}
level      = {{ grafana_config__log__level }}

[log.console]
level = {{ grafana_config__log__console__level }}

[log.file]
level           = {{ grafana_config__log__file__level }}
log_rotate      = {{ grafana_config__log__file__log_rotate }}
max_lines       = {{ grafana_config__log__file__max_lines }}
max_lines_shift = {{ grafana_config__log__file__max_lines_shift }}
daily_rotate    = {{ grafana_config__log__file__daily_rotate }}
max_days        = {{ grafana_config__log__file__max_days }}

[event_publisher]
enabled      = {{ grafana_config__event_publisher__enabled }}
rabbitmq_url = {{ grafana_config__event_publisher__rabbitmq_url }}
exchange     = {{ grafana_config__event_publisher__exchange }}

[dashboards.json]
enabled = {{ grafana_config__dashboards__json__enabled }}
path    = {{ grafana_config__dashboards__json__path }}
